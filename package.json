{
  "name": "the-power-of-typescript",
  "version": "1.0.0",
  "description": "Various demos to show the true power of TypeScript",
  "main": "build/index.js",
  "scripts": {
    "format": "prettier --write '**/*'",
    "format-check": "prettier --check '**/*'",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}'",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "tests": "jest --runInBand --coverage",
    "test": "npm run typecheck; npm run format-check && npm run lint && npm run tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/async-2023-08-04-the-power-of-typescript.git"
  },
  "keywords": [
    "async",
    "asyncjs",
    "talk",
    "typescript",
    "types"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/async-2023-08-04-the-power-of-typescript/issues"
  },
  "homepage": "https://github.com/jakesidsmith/async-2023-08-04-the-power-of-typescript#readme",
  "dependencies": {
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1"
  }
}
